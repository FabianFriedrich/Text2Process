        <semType name="Sentient" ID="5"/>
    </FE>
    <FE bgColor="008000" fgColor="FFFFFF" coreType="Peripheral" cBy="664" cDate="08/14/2001 01:25:51 PDT Tue" abbrev="Place" name="Place" ID="1613">
